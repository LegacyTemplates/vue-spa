{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "dev": "vue-cli-service build --watch",
    "serve": "vue-cli-service serve",
    "dtos": "x typescript",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "publish": "vue-cli-service build && dotnet publish -c Release",
    "test": "vue-cli-service test:unit",
    "test-watch": "vue-cli-service test:unit --watchAll"
  },
  "dependencies": {
    "@servicestack/client": "^1.0.54",
    "@servicestack/vue": "^1.0.13",
    "bootstrap": "^5.0.1",
    "core-js": "^3.12.1",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.2.0",
    "sass": "^1.34.0",
    "sass-loader": "^10.0.5",
    "typescript": "~4.2.4",
    "vue-template-compiler": "^2.6.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/bin/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest",
      "^@servicestack/vue$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(babel-jest|jest-vue-preprocessor)/)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "globals": {
      "testURL": "https://localhost:5001/"
    }
  }
}
