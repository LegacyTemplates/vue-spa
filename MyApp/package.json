{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm run build-vendor && npm run build",
    "dev": "webpack --config webpack.config.js --watch",
    "dev-server": "webpack-dev-server",
    "dtos": "cd src && typescript-ref",
    "build": "webpack --config webpack.config.js",
    "build-prod": "webpack --config webpack.config.js --env.prod",
    "build-vendor": "webpack --config webpack.config.vendor.js",
    "publish": "webpack --config webpack.config.vendor.js --env.prod && npm run build-prod && dotnet publish -c Release",
    "test": "karma start karma.conf.js",
    "test-watch": "karma start karma.conf.js --no-single-run --auto-watch"
  },
  "karma": {
    "globals": {
      "BaseUrl": "http://localhost:5000"
    }
  },
  "dependencies": {
    "@servicestack/client": "^1.0.14",
    "bootstrap": "^4.1.1",
    "es6-shim": "^0.35.3",
    "font-awesome": "^4.7.0",
    "vue": "^2.5.16",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@servicestack/cli": "^1.0.5",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.1",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "autoprefixer": "^8.6.4",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "gulp": "^3.9.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^2.0.4",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.15",
    "postcss-loader": "^2.1.5",
    "precss": "^3.1.2",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.11.1",
    "run-sequence": "^2.2.1",
    "sass-loader": "^7.0.3",
    "sinon": "^6.0.1",
    "sinon-chai": "^3.2.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^3.5.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.3",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "vue-ts-loader": "0.0.3",
    "webpack": "^3.12.0",
