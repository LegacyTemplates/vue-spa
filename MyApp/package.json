{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm run build-vendor && npm run build",
    "dev": "webpack --config webpack.config.js --watch",
    "dev-server": "webpack-dev-server",
    "dtos": "cd src && typescript-ref",
    "build": "webpack --config webpack.config.js",
    "build-prod": "webpack --config webpack.config.js --env.prod",
    "build-vendor": "webpack --config webpack.config.vendor.js",
    "publish": "webpack --config webpack.config.vendor.js --env.prod && npm run build-prod && dotnet publish -c Release",
    "test": "karma start karma.conf.js",
    "test-watch": "karma start karma.conf.js --no-single-run --auto-watch"
  },
  "karma": {
    "globals": {
      "BaseUrl": "http://localhost:5000"
    }
  },
  "dependencies": {
    "@servicestack/client": "^1.0.2",
    "bootstrap": "^4.0.0",
    "es6-shim": "^0.35.3",
    "font-awesome": "^4.7.0",
    "vue": "^2.5.13",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@servicestack/cli": "^1.0.2",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.47",
    "@types/node": "^9.4.0",
    "@types/sinon": "^4.1.3",
    "@types/sinon-chai": "^2.7.29",
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.2.5",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "karma": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^2.0.9",
    "lodash": "^4.17.4",
    "mocha": "^5.0.0",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.15",
    "postcss-loader": "^2.0.10",
    "precss": "^3.1.0",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.10.1",
    "run-sequence": "^2.2.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.14.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.1",
    "ts-loader": "^3.3.1",
    "typescript": "^2.6.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.1.1",
    "vue-template-compiler": "^2.5.13",
    "vue-ts-loader": "0.0.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.11.1"
  }
